{
  "compilerOptions": {
   "target": "ES2021",
   "module": "commonjs",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "jsx": "react-jsx"
  },
  "exclude": [
    "node_modules",
    "dist",
    "public",
    "out"
  ]
}
